{
  "name": "@classmatejs/factory",
  "description": "Headless classmate factory to compute class names and variants without rendering components.",
  "version": "0.0.5",
  "keywords": [
    "classmate",
    "headless",
    "factory",
    "typescript",
    "dynamic-classnames",
    "dynamic-variants",
    "css",
    "utility-first",
    "classname",
    "tailwind"
  ],
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richard-unterberg/classmate.git"
  },
  "bugs": {
    "url": "https://github.com/richard-unterberg/classmate/issues"
  },
  "homepage": "https://github.com/richard-unterberg/classmate/#readme",
  "scripts": {
    "build": "echo \"@classmatejs/factory is muted\"",
    "test": "echo \"@classmatejs/factory is muted\"",
    "package": "echo \"@classmatejs/factory is muted\""
  }
}
